entrypoint = "README.md"
modules = ["nodejs-22", "python-3.11"]

[nix]
channel = "stable-24_05"

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8081
externalPort = 80

[[ports]]
localPort = 8082
externalPort = 3000

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Expo Dev Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --tunnel --port 5000"
waitForPort = 5000

[agent]
expertMode = true
integrations = ["twilio:1.0.0"]

[deployment]
deploymentTarget = "static"
run = ["npx", "serve", "dist", "-l", "5000"]
build = ["npx", "expo", "export", "--platform", "web"]
publicDir = "dist"

[userenv]

[userenv.shared]
EXPO_PUBLIC_API_URL = "https://gacha-travel--s8869420.replit.app"
